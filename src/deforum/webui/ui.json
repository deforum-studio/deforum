{
  "General Settings": {
    "settings_file": {
      "widget_type": "file_input",
      "accepted_file_types": [".txt"],
      "default": null,
      "label": "Upload Settings File",
      "accessible_name": "Settings File Upload"
    },
    "use_settings_file": {
      "widget_type": "checkbox",
      "default": false,
      "label": "Use Settings File",
      "accessible_name": "Use Settings File"
    },
    "prompts": {
      "label": "Prompts",
      "widget_type": "text box",
      "default": "",
      "accessible_name": ""
    },
    "animation_prompts_negative": {
      "label": "Negative Prompts",
      "widget_type": "text box",
      "default": "",
      "accessible_name": ""
    },
    "keyframes": {
      "label": "Keyframes",
      "widget_type": "text box",
      "default": "",
      "accessible_name": ""
    },
    "width": {
      "widget_type": "number",
      "default": 1024,
      "min": 512,
      "max": 4096,
      "label": "Width",
      "accessible_name": "Image Width"
    },
    "height": {
      "widget_type": "number",
      "default": 576,
      "min": 256,
      "max": 2160,
      "label": "Height",
      "accessible_name": "Image Height"
    },
    "steps": {
      "widget_type": "number",
      "default": 19,
      "min": 1,
      "max": 256,
      "label": "Steps",
      "accessible_name": "Diffusion Steps"
    },
    "show_info_on_ui": {
      "widget_type": "dropdown",
      "options": [true, false],
      "default": true,
      "label": "Show Info on UI",
      "accessible_name": "Toggle Information Display on UI"
    },
    "seed": {
      "widget_type": "number",
      "default": -1,
      "min": -1,
      "max": 1000000,
      "label": "Seed",
      "accessible_name": "Random Seed for Generation"
    },
    "sampler_name": {
      "widget_type": "dropdown",
      "options": [
                  "DPM++ 2M",
                  "DPM++ 2M Karras",
                  "DPM++ 2M SDE",
                  "DPM++ 2M SDE Karras",
                  "DPM++ 2S a",
                  "DPM++ 2S a Karras",
                  "DPM++ SDE",
                  "DPM++ SDE Karras",
                  "DPM2",
                  "DPM2 Karras",
                  "DPM2 a",
                  "DPM2 a Karras",
                  "DPM adaptive",
                  "DPM fast",
                  "Euler",
                  "Euler a",
                  "Heun",
                  "LMS",
                  "LMS Karras",
                  "Deis",
                  "UniPcMultiStep",
                  "LCM",
                  "TCD"
              ],
      "default": "DPM++ 2M SDE Karras",
      "label": "Sampler",
      "accessible_name": "Sampling Algorithm"
    },
    "batch_name": {
      "widget_type": "text input",
      "default": "Classic-3D-Motion",
      "label": "Batch Name",
      "accessible_name": "Name of the Batch for Identification"
    }
  },
  "Animation & Motion": {
    "animation_mode": {
      "widget_type": "dropdown",
      "options": ["2D", "3D", "None"],
      "default": "3D",
      "label": "Animation Mode",
      "accessible_name": "Type of Animation"
    },
    "max_frames": {
      "widget_type": "number",
      "default": 362,
      "min": 1,
      "max": 1000,
      "label": "Max Frames",
      "accessible_name": "Maximum Number of Animation Frames"
    },
    "translation_x": {
      "widget_type": "text input",
      "default": "0: (0)",
      "label": "Translation X",
      "accessible_name": "X-axis Translation Formula"
    },
    "translation_y": {
      "widget_type": "text input",
      "default": "0: (0)",
      "label": "Translation Y",
      "accessible_name": "Y-axis Translation Formula"
    },
    "translation_z": {
      "widget_type": "text input",
      "default": "0: (0)",
      "label": "Translation Z",
      "accessible_name": "Z-axis Translation Formula"
    },
    "rotation_x": {
      "widget_type": "text input",
      "default": "0: (0)",
      "label": "Rotation X",
      "accessible_name": "X-axis Rotation Formula"
    },
    "rotation_y": {
      "widget_type": "text input",
      "default": "0: (0)",
      "label": "Rotation Y",
      "accessible_name": "Y-axis Rotation Formula"
    },
    "rotation_z": {
      "widget_type": "text input",
      "default": "0: (0)",
      "label": "Rotation Z",
      "accessible_name": "Z-axis Rotation Formula"
    }

  },

  "Diffusion Schedules": {
    "strength_schedule": {
      "widget_type": "text input",
      "default": "0: (0.65)",
      "label": "Strength Schedule",
      "accessible_name": "Strength Schedule"
    },
    "noise_schedule": {
      "widget_type": "text input",
      "default": "0: (0.02)",
      "label": "Noise Schedule",
      "accessible_name": "Noise Schedule"
    },
    "cfg_scale_schedule": {
      "widget_type": "text input",
      "default": "0: (5.0)",
      "label": "CFG Scale Schedule",
      "accessible_name": "CFG Scale Schedule"
    },
    "subseed_schedule": {
      "widget_type": "text input",
      "default": "0: (0)",
      "label": "Subseed Schedule",
      "accessible_name": "Subseed Schedule"
    },
    "subseed_strength_schedule": {
      "widget_type": "text input",
      "default": "0: (0.0)",
      "label": "Subseed Strength Schedule",
      "accessible_name": "Subseed Strength Schedule"
    },

    "enable_subseed_scheduling": {
      "widget_type": "checkbox",
      "default": true,
      "label": "Use Subseed Strength Schedule",
      "accessible_name": "Use Subseed Strength Schedule"
    },


    "parseq_manifest": {
      "widget_type": "text input",
      "default": "",
      "label": "ParSeq Manifest",
      "accessible_name": "ParSeq"
    }
  },

  "Image & Mask Settings": {
    "use_init": {
      "widget_type": "dropdown",
      "options": [true, false],
      "default": false,
      "label": "Use Initial Image",
      "accessible_name": "Use an Initial Image for Generation"
    },
    "init_image": {
      "widget_type": "text input",
      "default": "",
      "label": "Initial Image URL",
      "accessible_name": "URL of the Initial Image"
    },
    "use_mask": {
      "widget_type": "dropdown",
      "options": [true, false],
      "default": false,
      "label": "Use Mask",
      "accessible_name": "Use a Mask for Generation"
    },
    "mask_file": {
      "widget_type": "text input",
      "default": "https://deforum.github.io/a1/M1.jpg",
      "label": "Mask File URL",
      "accessible_name": "URL of the Mask File"
    },
    "overlay_mask": {
      "widget_type": "dropdown",
      "options": [true, false],
      "default": true,
      "label": "Overlay Mask",
      "accessible_name": "Overlay Mask Over the Image"
    }
  }
}
